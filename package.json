{
  "name": "vite-plugin-laravel-translations",
  "version": "0.1.2",
  "private": false,
  "description": "Vite Plugin to make Laravel Translations globally available as JSON field.",
  "author": "Dcode Group",
  "main": "dist/vite.js",
  "scripts": {
    "clear": "rimraf dist && mkdirp dist",
    "build": "pnpm run clear && tsc",
    "build:dev": "pnpm run clear && vite",
    "build:prod": "pnpm run clear && vite build",
    "lint": "eslint . --ext js,jsx,vue,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,vue,ts,tsx --fix"
  },
  "keywords": [
    "vite",
    "plugin",
    "laravel",
    "translations",
    "vue",
    "react",
    "json",
    "php"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^8.21.3",
    "@types/glob": "^8.1.0",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^8.1.0",
    "mkdirp": "^1.0.4",
    "path": "^0.12.7",
    "php-parser": "^3.1.3",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.5",
    "vite": "^4.2.0"
  },
  "dependencies": {
    "php-array-reader": "^1.3.5"
  },
  "peerDependencies": {
    "vite": "^4.0.0"
  },
  "bugs": {
    "url": "https://github.com/dcodegroup/vite-plugin-laravel-translations/issues"
  },
  "engines": {
    "node": ">= 12.0.0",
    "pnpm": ">= 7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcodegroup/vite-plugin-laravel-translations.git"
  }
}
